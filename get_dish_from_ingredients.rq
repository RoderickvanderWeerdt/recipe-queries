#+ tags:
#+   - ingredients
#+ defaults:
#+   - ingredient: Kaas

PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX vocab: <https://nimbostratus.nl/vocab/>
PREFIX recept: <http://nimbostratus.nl/recept/>
PREFIX ob: <http://nimbostratus.nl/>

SELECT ?gerechtnaam WHERE {
  ?gerecht vocab:dishName ?gerechtnaam .
  ?ingredient_node vocab:usedIn ?gerecht .
  ?ingredient_node vocab:ingredient ?_ingredient .
} LIMIT 10
